{
    "name": "sunscreem/laravel-flickr-gallery",
    "description": "Simple way to connect a flickr album to your website",
    "authors": [
        {
            "name": "Rob Cooper",
            "email": "sunscreem@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "illuminate/console": "^9.*",
        "jeroen-g/flickr": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Sunscreem\\LaravelFlickrGallery\\": "src"
        }
    },
    "extra": {
